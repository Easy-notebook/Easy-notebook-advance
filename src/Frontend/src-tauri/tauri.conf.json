{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "easy-notebook",
  "version": "0.1.0",
  "identifier": "easy-notebook.silan.tech",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "easy-notebook(AI for data science)",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.svg",
      "icons/icon_256.ico",      
      "icons/icon_128.ico",      
      "icons/icon_64.ico",      
      "icons/icon_32.ico"      
    ]
  }
}
